{
  "name": "arthurbot",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "tsnd --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "main": "src/index.ts",
  "dependencies": {
    "@open-wa/wa-automate": "^4.76.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "emoji-regex": "^10.4.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jsdom": "^26.1.0",
    "moment-timezone": "^0.5.48",
    "mongoose": "^8.15.1",
    "prettier": "^3.5.3",
    "sqlite3": "^5.1.7",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.33.0",
    "user-agents": "^1.1.531"
  },
  "devDependencies": {
    "@types/sqlite3": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}